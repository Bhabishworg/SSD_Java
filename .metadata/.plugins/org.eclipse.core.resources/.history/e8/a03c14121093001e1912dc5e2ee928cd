package lab6;

import java.util.*;

public class EmailStore {
	Set<String> emailAddresses = new HashSet<String>();

	boolean addEmail(String email) {
		if (emailAddresses.contains(email)) {
			emailAddresses.add(email);
			return false;
		} else {
			return true;
		}

	}
	
	boolean hasEmail(String email) {
		return true;
	}
}
